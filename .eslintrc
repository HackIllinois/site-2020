{
  "extends": ["react-app", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["components", "./src/components"]
        ],
        "extensions": [".js", ".jsx"]
      }
    }
  }
}